
# Sample rc file, please fill out and put under $HOME/.config/heduprc

# from address that will be used to send mails
from_address: username@hostname.edu

# gpg key deposited at Hetzner, will be used to sign mails
gpg_sign_key: AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA

# explicit gpg binary path
gpg_binary: gpg

# which account to user for hetzner
hetzner_account: K9999999999

# explicit mail binary path
mail_binary: mail

# vim: ft=yaml
